{
  "name": "horus_party",
  "version": "0.1.0",
  "private": true,
  "builds": {
    "src": "index.js",
    "use": "@now/node-server",
    "config": {
      "maxLambdaSize": "50mb"
    }
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "heroku": "^7.42.1",
    "http-proxy-middleware": "^1.0.4",
    "pino": "^6.3.2",
    "redis": "^3.0.2",
    "serve": "^11.3.2",
    "socket.io": "^2.3.0"
  },
  "scripts": {
    "dev": "run-p server start",
    "server": "node index.js",
    "start": "serve -s client/build",
    "build": "npm run build --prefix client",
    "test": "npm run test --prefix client",
    "eject": "npm run eject --prefix client",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "10.20.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "express-pino-logger": "^5.0.0",
    "node-env-run": "^3.0.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^1.6.1"
  }
}
